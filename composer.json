{
    "name": "daandelange/helpers",
    "type": "kirby-plugin",
    "packageManager": "pnpm",
    "description": "My personal helpers for Kirby",
    "homepage": "https://github.com/Daandelange/kirby-helpers",
    "version": "0.2.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Daan de Lange",
            "role": "Developer"
        }
    ],
    "keywords": [
        "kirby5",
        "kirby-plugin"
    ],
    "require": {
        "getkirby/composer-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Daandelange\\Helpers\\": "src/"
        }
    },
    "scripts": {
        "dist": "composer install --no-dev --optimize-autoloader"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "extra": {
        "installer-name": "daans-helpers"
    }
}
